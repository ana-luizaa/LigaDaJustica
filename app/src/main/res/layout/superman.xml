<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="364dp"
        android:layout_height="615dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/txtNomeS"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:fontFamily="serif"
            android:text="@string/txtNome4"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/txtCodnS"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:fontFamily="serif"
            android:text="@string/Codn4"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/txtEspecieS"
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:fontFamily="serif"
            android:text="@string/txtEspecie4"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/txtHabilidadeS"
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:fontFamily="serif"
            android:text="@string/txtHabilidade4"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/txtEquipS"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:fontFamily="serif"
            android:text="@string/txtEquipamento4"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/txtVulS"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:fontFamily="serif"
            android:text="@string/txtVulnerabilidade4"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/txtNivelS"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:fontFamily="serif"
            android:text="@string/txtAcesso4"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#000000"
            android:textSize="14sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="358dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <Button
                    android:id="@+id/btnVoltar4"
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:onClick="voltar"
                    android:text="@string/btnVoltar" />

            </LinearLayout>

            <ImageView
                android:id="@+id/imgSuper"
                android:layout_width="200dp"
                android:layout_height="181dp"
                android:contentDescription="@string/descMulher"
                app:srcCompat="@drawable/superman" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>